{
	"name": "template-angular",
	"version": "0.0.0",
	"scripts": {
		"dev": "concurrently --names 'app,test' 'yarn start:dev' 'yarn test:dev'",
		"start": "ng serve",
		"start:dev": "ng serve -o",
		"test": "jest --coverage",
		"test:dev": "jest --watch",
		"format": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
		"format:fix": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
		"lint": "eslint . --ext .js,.ts",
		"lint:fix": "yarn lint -- --fix",
		"build": "yarn build:clean && ng build --base-href ./",
		"build:clean": "rimraf dist; exit 0",
		"docs:clean": "rimraf docs; exit 0",
		"copy:app": "copyfiles dist/template-angular/browser/* ./docs -f",
		"build:github": "yarn docs:clean && yarn build && yarn copy:app",
		"type-check": "tsc --pretty --noEmit",
		"prepare": "husky"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^17.3.0",
		"@angular/common": "^17.3.0",
		"@angular/compiler": "^17.3.0",
		"@angular/core": "^17.3.0",
		"@angular/forms": "^17.3.0",
		"@angular/platform-browser": "^17.3.0",
		"@angular/platform-browser-dynamic": "^17.3.0",
		"@angular/router": "^17.3.0",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.14.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^17.3.7",
		"@angular-eslint/builder": "17.4.1",
		"@angular-eslint/eslint-plugin": "17.4.1",
		"@angular-eslint/eslint-plugin-template": "17.4.1",
		"@angular-eslint/schematics": "17.4.1",
		"@angular-eslint/template-parser": "17.4.1",
		"@angular/cli": "^17.3.7",
		"@angular/compiler-cli": "^17.3.0",
		"@types/jest": "^29.5.12",
		"@typescript-eslint/eslint-plugin": "7.8.0",
		"@typescript-eslint/parser": "7.8.0",
		"concurrently": "^8.2.2",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-simple-import-sort": "^12.1.0",
		"eslint-plugin-unicorn": "^53.0.0",
		"husky": "^9.0.11",
		"jest": "^29.7.0",
		"jest-preset-angular": "^14.0.4",
		"prettier": "^3.2.5",
		"typescript": "~5.4.2"
	}
}
